group{
   images.image: "e_wallpaper_thumb.png" COMP;
      name: "icon";
      max: 24 24;
      parts {
          part {
             name: "image";
             mouse_events: 0;
             type: IMAGE;
             description {
                  state: "default" 0.0;
                  image.normal: "e_wallpaper_thumb.png";
             }
          }
      }
}

group { name: "e/desktop/background";
  //~ data.item: "noanimation" "0";
  images.image: "e_wallpaper.png" COMP;
  data { item: "style" "4"; }
  max: 2062 1124;

  parts {
    part { name: "bg"; mouse_events: 0; type: IMAGE;
      description { state: "default" 0.0;
      color: 0 0 0 255;
      aspect: 1.600000000 1.600000000; 
      aspect_preference: NONE;
      image {
          normal: "e_wallpaper.png";
          scale_hint: STATIC;
        }
      }
      description { state: "show" 0.0;
      inherit: "default";
      color: 255 255 255 255;
      }
    }
  }
  programs {
      program {
         name:   "start";
         signal: "show"; source: "";
         action: STATE_SET "default" 0.0;
         transition: LINEAR 1.5;
         target: "bg";
         after:  "show";
      }
      program {
         name:   "show";
         action: STATE_SET "show" 0.0;
         transition: LINEAR 1.5;
         target: "bg";
         after: "end";
      }
      program {
         name:   "end";
         action: SIGNAL_EMIT "e,state,done" "";
      }
  }
}
